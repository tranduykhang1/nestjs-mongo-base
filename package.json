  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "start": "nest start",
    "start:dev": "NODE_ENV=dev nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "docker:test": "docker compose exec api npm run test",
    "test": "NODE_ENV=test jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_ENV=e2e-test jest --config  ./tests/jest-e2e.json --forceExit",
    "seed": "docker compose exec api node dist/src/seed",
    "seed:refresh": "docker compose exec api node dist/src/seed --refresh",
    "prepare": "husky install"
  },
  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "@nestjs-modules/ioredis": "^2.0.2",
    "@nestjs-modules/mailer": "^2.0.2",
